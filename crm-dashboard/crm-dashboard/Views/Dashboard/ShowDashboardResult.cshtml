@model DashboardData

@{
    Layout = "_Layout"; // or null if you don't have a layout
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard Result</title>
</head>
<body>
<h1>Dashboard Result</h1>

@if (Model != null)
{
    <h3>Dashboard Information:</h3>
    <ul>
        <li><strong>Leads Count:</strong> @Model.Leads.Count</li>
        <li><strong>Tickets Count:</strong> @Model.Tickets.Count</li>
        <li><strong>Budget Total:</strong> @Model.Budgets.Count</li>
        <!-- Add more properties here depending on your DashboardData structure -->
    </ul>
}
else
{
    <p>No data found in session.</p>
}

</body>
